<Page x:Class="AtmUi.Pages.CashToPaperWallet"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
    xmlns:data="clr-namespace:System.Windows.Data;assembly=PresentationFramework"
    xmlns:local="clr-namespace:AtmUi.Pages" 
    xmlns:viewmodels="clr-namespace:AtmCommon.ViewModels"
    xmlns:templates="clr-namespace:AtmCommon.Templates"
    xmlns:pages="clr-namespace:AtmUi.Pages"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" 
    d:DesignHeight="650" d:DesignWidth="1080"
    Title="{x:Static local:CashToPaperWallet.LinkName}"
    >
    <templates:OptionScreen Loaded="OptionScreen_Loaded"/>
    <Page.DataContext>
        <viewmodels:OptionScreen
            PageTitle="{x:Static local:CashToPaperWallet.LinkName}" 
            SystemButton_Link="{StaticResource SystemButtonLink}"
            Description=""
            />
    </Page.DataContext>
</Page>
